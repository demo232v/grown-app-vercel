{
    "version": 2,
    "routes": [
        {
            "src": "/secret.js",
            "headers": {
                "Content-Type": "application/javascript",
                "Cache-Control": "no-store, max-age=0"
            }
        },
        {
            "src": "/.*",
            "dest": "/404.html",
            "status": 404
        }
    ]
}
