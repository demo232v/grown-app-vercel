{
  "version": 2,
  "routes": [
    {
      "src": "/secure-app.js",
      "headers": {
        "Content-Type": "application/javascript",
        "Cache-Control": "no-store, max-age=0",
        "X-Content-Type-Options": "nosniff"
      }
    },
    {
      "src": "/.*",
      "dest": "/404.html",
      "status": 404
    }
  ]
}
