{
  "version": 2,
  "routes": [
    {
      "src": "/secret.js",
      "headers": {
        "Content-Type": "application/javascript",
        "Cache-Control": "no-store"
      }
    },
    {
      "src": "/loader.js",
      "headers": {
        "Content-Type": "application/javascript",
        "Cache-Control": "no-store"
      }
    },
    {
      "src": "/.*",
      "dest": "/404.html",
      "status": 404
    }
  ]
}
